<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".EditProductFragment">

    <EditText
        android:id="@+id/imageUrlProduct_et_EditProductFragment2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:layout_marginEnd="25dp"
        android:ems="10"
        android:fontFamily="sans-serif-black"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toBottomOf="@+id/nameProduct_TV_EditProductFragment4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/nameProduct_TV_EditProductFragment4"
        app:layout_constraintTop_toTopOf="@+id/nameProduct_TV_EditProductFragment4" />

    <TextView
        android:id="@+id/nameProduct_TV_EditProductFragment4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="25dp"
        android:layout_marginTop="30dp"
        android:fontFamily="@font/emblema_one"
        android:text="URL image: "
        android:textSize="25sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/nameProduct_TV_EditProductFragment3" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="245dp"
        android:layout_height="198dp"
        android:layout_marginStart="25dp"
        android:layout_marginTop="25dp"
        android:src="@drawable/avatar_icon"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/nameProduct_et_EditProductFragment"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:layout_marginEnd="25dp"
        android:ems="10"
        android:fontFamily="sans-serif-black"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toBottomOf="@+id/nameProduct_TV_EditProductFragment"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/nameProduct_TV_EditProductFragment"
        app:layout_constraintTop_toTopOf="@+id/nameProduct_TV_EditProductFragment" />

    <EditText
        android:id="@+id/caratProduct_et_EditProductFragment"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:layout_marginEnd="25dp"
        android:ems="10"
        android:fontFamily="sans-serif-black"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toBottomOf="@+id/nameProduct_TV_EditProductFragment2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/nameProduct_TV_EditProductFragment2"
        app:layout_constraintTop_toTopOf="@+id/nameProduct_TV_EditProductFragment2" />

    <EditText
        android:id="@+id/priceProduct_et_EditProductFragment"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:layout_marginEnd="25dp"
        android:ems="10"
        android:fontFamily="sans-serif-black"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toBottomOf="@+id/nameProduct_TV_EditProductFragment3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/nameProduct_TV_EditProductFragment3"
        app:layout_constraintTop_toTopOf="@+id/nameProduct_TV_EditProductFragment3"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/nameProduct_TV_EditProductFragment"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="25dp"
        android:layout_marginTop="35dp"
        android:fontFamily="@font/emblema_one"
        android:text="Name: "
        android:textSize="25sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView2" />

    <TextView
        android:id="@+id/nameProduct_TV_EditProductFragment2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="25dp"
        android:layout_marginTop="30dp"
        android:fontFamily="@font/emblema_one"
        android:text="Carats: "
        android:textSize="25sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/nameProduct_TV_EditProductFragment" />

    <TextView
        android:id="@+id/nameProduct_TV_EditProductFragment3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="25dp"
        android:layout_marginTop="30dp"
        android:fontFamily="@font/emblema_one"
        android:text="Price: "
        android:textSize="25sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/nameProduct_TV_EditProductFragment2" />

    <Button
        android:id="@+id/updateBtn_editProductFragment"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Update ! "
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/nameProduct_TV_EditProductFragment4"
        app:layout_constraintVertical_bias="0.366" />
</androidx.constraintlayout.widget.ConstraintLayout>